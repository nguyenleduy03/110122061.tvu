        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Dữ liệu chiều cao\n",
        "height_male = [175, 180, 178, 165, 177, 185, 170, 168, 174, 182]\n",
        "height_female = [160, 162, 158, 155, 165, 168, 150, 158, 160, 162]\n",
        "\n",
        "# Vẽ histogram\n",
        "plt.hist(height_male, alpha=0.5, label='Nam', bins=5, color='blue', edgecolor='black')\n",
        "plt.hist(height_female, alpha=0.5, label='Nữ', bins=5, color='red', edgecolor='black')\n",
        "\n",
        "# Cài đặt cho biểu đồ\n",
        "plt.xlabel('Chiều cao (cm)')\n",
        "plt.ylabel('Tần suất')\n",
        "plt.legend(loc='upper right')\n",
        "plt.title('Phân bố chiều cao của nam và nữ')\n",
        "\n",
        "# Hiển thị biểu đồ\n",
        "plt.show()\n"
